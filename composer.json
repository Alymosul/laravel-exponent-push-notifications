{
    "name": "tomasz/laravel-exponent-push-notifications",
    "description": "Exponent push notifications driver for laravel",
    "homepage": "https://github.com/tomasz/laravel-exponent-push-notifications",
    "license": "MIT",
    "authors": [
        {
            "name": "Tomasz Niedzielski",
            "email": "tomasz9531@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "tomasz/exponent-server-sdk-php": "1.1.*",
        "laravel/framework": "^5.6 | ^6.0 | ^7.0"
    },
    "require-dev": {
        "mockery/mockery": "^0.9.5",
        "phpunit/phpunit": "~5.4||~5.7||~6.0||^7.0",
        "orchestra/testbench": "~3.3||~4.0"
    },
    "autoload": {
        "psr-4": {
            "NotificationChannels\\ExpoPushNotifications\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "NotificationChannels\\ExpoPushNotifications\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "NotificationChannels\\ExpoPushNotifications\\ExpoPushNotificationsServiceProvider"
            ]
        }
    }
}
